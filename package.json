{
  "name": "js_dev-env",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "prestart": "babel-node scripts/messages/prestart.js",
    "start": "run-p start:* lint:watch test:watch",
    "start:server": "PORT=3000 babel-node scripts/server.js",
    "share": "run-p start share:lt",
    "share:lt": "PORT=3000 sh -c 'lt --port ${PORT} --subdomain develop'",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "test": "jest ./src",
    "test:watch": "npm test -- --watch"
  },
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "7.13.8",
    "@babel/register": "7.13.8",
    "babel-loader": "8.2.2",
    "chalk": "4.1.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "css-loader": "5.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "express": "4.17.1",
    "faker": "^5.4.0",
    "html-webpack-plugin": "5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "json-schema-faker": "0.5.0-rcv.27",
    "json-server": "0.16.3",
    "localtunnel": "2.0.1",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "open": "7.4.2",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "surge": "0.21.7",
    "webpack": "5.24.2",
    "webpack-dev-middleware": "4.1.0"
  }
}
